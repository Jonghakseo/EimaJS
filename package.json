    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "figlet": "^1.5.2",
    "gradient-string": "^2.0.0",
    "ink": "^3.2.0",
